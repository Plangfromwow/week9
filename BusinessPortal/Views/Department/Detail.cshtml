@model Department;
@{
    ViewData["Title"] = "Detail";
}

<h1>Department Detail</h1>

Department Code: @Model.id <br />
Department Name: @Model.name <br/>
Department Location: @Model.location <br />

<h2>Employee's in @Model.name</h2>
@foreach(Employee emp in (List<Employee>)ViewData["employees"]){
    <div>@emp.firstname @emp.lastname</div>
}